{
  "name": "@typescript-plus/builtin-class-decorator",
  "version": "0.4.0",
  "description": "A TypeScript class decorator for extending builtin classes in ES5.",
  "repository": "typescript-plus/builtin-class-decorator",
  "license": "MIT",
  "author": "mosop",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dist": "run-s dist:clean dist:build",
    "dist:build": "run-p dist:build:*",
    "dist:build:es5": "tsc -p tsconfig.dist.es5.json",
    "dist:build:es6": "tsc -p tsconfig.dist.es6.json",
    "dist:clean": "rm -rf dist",
    "doc": "typedoc --mode file --excludeNotExported --out docs/api src",
    "lint": "eslint \"{src,spec}/**/*.{ts,js}\"",
    "posttest": "npm run lint",
    "test": "run-s test:compile test:run",
    "test:compile": "run-p test:compile:*",
    "test:compile:es5": "run-p test:compile:es5:*",
    "test:compile:es6": "run-p test:compile:es6:*",
    "test:compile:es5:es5": "tsc -p tsconfig.build.es5.json --outDir build/es5",
    "test:compile:es5:es6": "tsc -p tsconfig.build.es5.json --outDir build/es6",
    "test:compile:es5:spec": "tsc -p tsconfig.spec.es5.json",
    "test:compile:es6:es5": "tsc -p tsconfig.build.es6.json --outDir build/es5",
    "test:compile:es6:es6": "tsc -p tsconfig.build.es6.json --outDir build/es6",
    "test:compile:es6:spec": "tsc -p tsconfig.spec.es6.json",
    "test:run": "run-s test:run:*",
    "test:run:es5": "jasmine 'build/es5/spec/**/*.spec.js'",
    "test:run:es6": "jasmine 'build/es6/spec/**/*.spec.js'",
    "testx": "npm run test:compile && jasmine"
  },
  "devDependencies": {
    "@myowncake/config": "^0.2.1",
    "@types/jasmine": "^3.9.1",
    "@types/node": "^14.17.20",
    "@types/tmp": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jasmine": "^3.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  }
}
