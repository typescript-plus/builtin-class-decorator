{
  "author": "mosop",
  "description": "A TypeScript class decorator for extending builtin ES5 classes.",
  "devDependencies": {
    "@types/jasmine": "^2.8.7",
    "@types/node": "^8.10.17",
    "@types/tmp": "0.0.33",
    "jasmine": "^2.99.0",
    "npm-run-all": "^4.1.3",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3"
  },
  "license": "MIT",
  "main": "dist/src/index.js",
  "name": "@typescript-plus/builtin-class-decorator",
  "scripts": {
    "dist": "npm-run-all dist:clean dist:build",
    "dist:build": "tsc -p tsconfig.dist.json",
    "dist:clean": "rm -rf dist",
    "doc": "typedoc --mode file --excludeNotExported --out docs/api src",
    "lint": "tslint -p . -t verbose",
    "posttest": "npm run lint",
    "test": "ts-node node_modules/jasmine/bin/jasmine 'spec/**/*.spec.ts'"
  },
  "types": "dist/src/index.d.js",
  "version": "0.1.0"
}
