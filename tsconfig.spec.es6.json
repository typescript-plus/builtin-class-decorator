{
  "$schema": "https://schemastore.azurewebsites.net/schemas/json/tsconfig.json",
  "extends": "./node_modules/@myowncake/config/dist/tsconfig.general.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build/es6",
    "module": "CommonJS",
    "target": "ES6",
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo.spec.es6"
  },
  "files": [
    "src/index.ts"
  ],
  "include": [
    "spec/*.ts",
    "spec/**/*.ts"
  ]
}
